{ #category : 'initialization' }
MBoxElement >> backgroundPaint [ 
	^ Color lightGray
]

{ #category : 'accessing' }
MBoxElement >> box [

	^ box
]

{ #category : 'accessing' }
MBoxElement >> box: aMBox [

	box := aMBox.

	self initializeAnnouncements 
]

{ #category : 'public' }
MBoxElement >> click [

	self box isFlagged ifTrue: [ 
		self box board gameEnded ifTrue: [ 
			self background: Color white darker darker.
	self writeText ] ].

	self background: Color white darker darker.
	self writeText
]

{ #category : 'explosion' }
MBoxElement >> explode [

	| explosionCenter |
	explosionCenter := self position + ((self width / 2) @ (self height / 2)).

	0 to: 359 by: 12 do: [ :angle |
		| anElement relativeFinalPosition |
		relativeFinalPosition := 0 @ 400
			                         rotateBy: angle degreesToRadians
			                         about: 0 @ 0.
		anElement := self newExplosionStar.
		self topMostParent addChild: anElement.
		anElement
			position: explosionCenter;
			addAnimation: (BlTransformAnimation new
					 duration: 1 second;
					 transformDo: [ :t |
						 t translateBy: relativeFinalPosition.
						 t rotateBy: 180 ];
					 onFinishedDo: [
						 anElement removeFromParent ];
					 yourself) yourself ]
]

{ #category : 'accessing' }
MBoxElement >> flag [

	| pngElement |
	self box isClicked ifTrue: [ ^ self ].
	pngElement := BlElement new.
	pngElement background: (self class flag scaledToSize: self size).

	pngElement constraintsDo: [ :c |
		c horizontal matchParent.
		c vertical matchParent ].

	self box isFlagged
		ifFalse: [ self addChild: pngElement as: 'png' ]
		ifTrue: [ self removeChildWithId: 'png' ].
	self box flag
]

{ #category : 'initialization' }
MBoxElement >> initialize [

	super initialize.
	self background: self backgroundPaint.
	self geometry: (BlRoundedRectangleGeometry cornerRadius: 5).
	self layout: BlFrameLayout new.
	self clipChildren: false.
	self initializeEvents.
	
]

{ #category : 'initialization' }
MBoxElement >> initializeAnnouncements [

	self box announcer
		when: MBoxExplosionAnnouncement
		send: #mine
		to: self.
	self box announcer
		when: MBoxClickedAnnouncement
		send: #click
		to: self
]

{ #category : 'initialization' }
MBoxElement >> initializeEvents [

	self addEventHandlerOn: BlClickEvent do: [ :anEvent |
		anEvent
			ifPrimary: [ self box isFlagged ifFalse: [self box click ] ]
			secondary: [ self flag ]
			middle: [ ^ self ]
			other: [ ^ self ] ]
]

{ #category : 'accessing' }
MBoxElement >> mine [

	self background: (self class mine scaledToSize: self size).
	self explode
]

{ #category : 'explosion' }
MBoxElement >> newExplosionStar [

	| outerSize innerSize numberOfPoints angleStep vertices |
	outerSize := 20.
	innerSize := 8.
	numberOfPoints := (4 to: 7) atRandom.
	angleStep := 360.0 / numberOfPoints.

	vertices := OrderedCollection new.
	0 to: 360 by: angleStep do: [ :angle |
		vertices
			add: (0 @ innerSize
					 rotateBy: (angle - (angleStep * 0.5)) degreesToRadians
					 about: 0 asPoint);
			add:
				(0 @ outerSize rotateBy: angle degreesToRadians about: 0 asPoint);
			add: (0 @ innerSize
					 rotateBy: (angle + (angleStep * 0.5)) degreesToRadians
					 about: 0 asPoint) ].

	^ BlElement new
		  geometry: (BlPolygonGeometry vertices: vertices);
		  background: Color random;
		  constraintsDo: [ :c | c ignoreByLayout ];
		  yourself
]

{ #category : 'writing' }
MBoxElement >> writeText [

	| textElt |
	self box announcer unsubscribe: self.

	textElt := BlTextElement new.
	textElt constraintsDo: [ :c |
		c frame vertical alignCenter.
		c frame horizontal alignCenter ].
	textElt text:
		(self box board mineCountAroundBox: self box) asString asRopedText.
	textElt text fontSize: (self width min: self height).
	textElt text fontName: 'Source Sans Pro'.
	self addChild: textElt
]
